<script type="text/javascript">
function adminInit() {
    // Create new Chart.
    var dataOverTime = new gadash.Chart({
        'last-n-days': 30,
        'divContainer': 'dataOverTimeConfig',
        'type': 'LineChart',
        'query': {
          'dimensions': 'ga:date',
          'sort': 'ga:date',
          'metrics': 'ga:visitors, ga:visits, ga:pageviews',
          'ids' : gadash.tableId      
        },
        'chartOptions': {
            height: 300,
            legend: {position: 'bottom'},
            hAxis: {title:'Date'},
            curveType: 'function'
        }
    }).render();/*
    var dataOverTime2 = new gadash.Chart({
